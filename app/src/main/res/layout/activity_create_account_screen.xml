<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.createAccountScreen">

    <!-- Rectangle 23 -->

    <!-- Spliff -->


    <View
        android:id="@+id/rectangle1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/round_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <!-- Stay High. Stay Happy. Always. -->
    <TextView
        android:id="@+id/stay_high_s"
        android:layout_width="199dp"
        android:layout_height="153dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="86dp"
        android:layout_marginBottom="191dp"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="8sp"
        android:text="@string/stay_high_s"
        android:textAppearance="@style/stay_high_s"
        android:translationY="-3.78sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <!-- Rectangle 24 -->
    <!-- Join -->

    <View
        android:id="@+id/joinbtn"
        android:layout_width="125dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/ic_rectangle_24"
        app:layout_constraintEnd_toStartOf="@+id/loginbtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stay_high_s" />

    <TextView
        android:id="@+id/join"
        android:layout_width="36dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="3sp"
        android:text="@string/join"
        android:textAppearance="@style/join"
        android:translationY="-1.55sp"
        app:layout_constraintBottom_toBottomOf="@+id/joinbtn"
        app:layout_constraintEnd_toEndOf="@+id/joinbtn"
        app:layout_constraintStart_toStartOf="@+id/joinbtn"
        app:layout_constraintTop_toTopOf="@+id/joinbtn" />
    <!-- Rectangle 23 -->

    <View
        android:id="@+id/loginbtn"
        android:layout_width="125dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="60dp"
        android:layout_marginRight="60dp"
        android:background="@drawable/ic_rectangle_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stay_high_s" />

    <TextView
        android:id="@+id/log_in"
        android:layout_width="51dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="3sp"
        android:text="@string/log_in"
        android:textAppearance="@style/log_in"
        android:translationY="-1.55sp"
        app:layout_constraintBottom_toBottomOf="@+id/loginbtn"
        app:layout_constraintEnd_toEndOf="@+id/loginbtn"
        app:layout_constraintStart_toStartOf="@+id/loginbtn"
        app:layout_constraintTop_toTopOf="@+id/loginbtn" />

    <!-- google logo -->

    <!-- Continue with google -->


</androidx.constraintlayout.widget.ConstraintLayout>